{
  "name": "@vakac995/react-calendar",
  "version": "1.0.6",
  "type": "module",
  "description": "A flexible, customizable React calendar component with date range and time picker support",
  "author": "vakac995",
  "license": "MIT",
  "keywords": [
    "react",
    "calendar",
    "datepicker",
    "date-range",
    "timepicker",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vakac995/react-calendar.git"
  },
  "homepage": "https://github.com/vakac995/react-calendar#readme",
  "bugs": {
    "url": "https://github.com/vakac995/react-calendar/issues"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "vite --config vite.demo.config.ts",
    "build:lib": "vite build",
    "build:demo": "vite build --config vite.demo.config.ts",
    "build": "npm run build:lib && npm run build:demo",
    "preview": "vite preview --config vite.demo.config.ts",
    "lint": "eslint src --max-warnings 0",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css}\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "circular": "madge --circular --extensions ts,tsx src",
    "deps:graph": "madge --image deps-graph.svg --extensions ts,tsx src",
    "icons:generate": "node scripts/generate-icons.js",
    "fix": "npm run typecheck && npm run lint:fix && npm run format",
    "validate": "npm run fix && npm run lint && npm run format:check && npm run circular",
    "check": "npm run validate && npm run test && npm run build",
    "deploy:npm": "npm run build:lib && npm publish --access public",
    "deploy:cf": "npm run build:demo && wrangler pages deploy demo-dist --project-name=react-calendar-demo",
    "deploy:all": "npm run deploy:npm && npm run deploy:cf",
    "release": "npm run check && npm version patch && npm run deploy:all && git push && git push --tags",
    "release:minor": "npm run check && npm version minor && npm run deploy:all && git push && git push --tags",
    "release:major": "npm run check && npm version major && npm run deploy:all && git push && git push --tags",
    "prepublishOnly": "npm run validate && npm run build:lib"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "happy-dom": "^20.0.11",
    "madge": "^8.0.0",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "puppeteer": "^24.34.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.33.5",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.3",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^4.0.16",
    "wrangler": "^4.56.0"
  }
}